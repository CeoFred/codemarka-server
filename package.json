    "cookie-parser": "^1.4.5",
    "cors": "2.8.5",
    "date-fns": "2.8.1",
    "dotenv": "8.1.0",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "express-session": "1.17.0",
    "express-validator": "6.2.0",
    "geoip-lite": "1.3.8",
    "jest": "^26.6.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.19",
    "lusca": "1.6.1",
