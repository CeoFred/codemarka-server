{
  "name": "colab-server",
  "version": "0.1.0",
  "description": "API based server code for colab",
  "repository": {
    "type": "git",
    "url": "https://github.com/CeoFred/colab"
  },
  "engines": {
    "node": "12.8.1"
  },
  "author": "codemon",
  "license": "MIT",
  "scripts": {
    "prod-start": "set NODE_ENV=production && npm start",
    "start": "node dist/server.js",
    "build": "npm run build-ts && npm run lint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "test-dev": "set NODE_ENV=test && nodemon --watch . --ext ts --exec \"mocha -r ts-node/register tests/**/*.test.ts --timeout 10000\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"*/**/*.{js,ts}\" --quiet --fix",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@sendgrid/mail": "^7.3.0",
    "archiver": "3.1.1",
    "async": "3.1.0",
    "axios": "0.19.0",
    "bcryptjs": "2.4.3",
    "bluebird": "3.7.0",
